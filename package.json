{
  "name": "@yousolution/node-red-contrib-you-stream-upload",
  "version": "1.1.0",
  "description": "Node-RED nodes for streaming multipart uploads to PostgreSQL Large Objects and streaming downloads from PostgreSQL or filesystem directly to HTTP response.",
  "license": "MIT",
  "scripts": {
    "update1": "node scripts/update.js",
    "update2": "PACKAGE_NAME=$(npm pack) && mv $PACKAGE_NAME ./data && cd data && rm -rf node_modules && npm i $PACKAGE_NAME  && docker-compose restart",
    "update": "PACKAGE_NAME=$(npm pack) && rm -rf ./data/node_modules && cd data && npm i ../$PACKAGE_NAME && docker-compose restart",
    "test": "mocha 'test/**/*.spec.js'",
    "coverage": "nyc npm run test"
  },
  "keywords": [
    "node-red",
    "chunk",
    "upload",
    "stream",
    "postgres",
    "postgresql",
    "large object",
    "download",
    "response",
    "filesystem",
    "youSolution.Cloud"
  ],
  "author": "Andrea Trentin <andrea.trentin@yousolution.cloud>",
  "node-red": {
    "version": ">=2.0.0",
    "nodes": {
      "stream-upload": "./nodes/stream-upload.js",
      "stream-download": "./nodes/stream-download.js",
      "pg-config": "./nodes/pg-config.js",
      "pg-stream-upload": "./nodes/pg-stream-upload.js",
      "pg-stream-download": "./nodes/pg-direct-download.js",
      "file-stream-upload": "./nodes/file-stream-upload.js",
      "file-direct-download": "./nodes/file-direct-download.js",
      "pg-response-stream": "./nodes/pg-response-stream.js",
      "file-response-stream": "./nodes/file-response-stream.js"
    }
  },
  "dependencies": {
    "cookie": "^1.0.2",
    "cross-conf-env": "^1.3.0",
    "dotenv": "^17.2.1",
    "busboy": "^1.6.0",
    "pg": "^8.11.1",
    "pg-large-object": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yousolution-cloud/node-red-contrib-you-upload-stream.git"
  },
  "bugs": {
    "url": "https://github.com/yousolution-cloud/node-red-contrib-you-upload-stream/issues"
  },
  "homepage": "https://github.com/yousolution-cloud/node-red-contrib-you-upload-stream#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "node-red": "^3.0.2",
    "node-red-node-test-helper": "^0.3.1"
  }
}